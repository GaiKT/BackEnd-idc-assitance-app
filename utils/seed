CREATE TABLE checklistFdc (
    id INT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
    -- fdc
    fdc_phase1 FLOAT NOT NULL,
    fdc_phase2 FLOAT NOT NULL,
    -- fdc comment
    fdc_comment VARCHAR(255),
    -- create time
    created_at TIMESTAMP NOT NULL,
    updated_at TIMESTAMP NOT NULL,
    -- user 
    user_id INT,
    FOREIGN KEY (user_id) REFERENCES users(user_id)  
);

CREATE TABLE checklistTransformer (
    id INT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,    
    -- rmu 
    sw_in1 BOOLEAN NOT NULL,
    sw_ground_in1 BOOLEAN NOT NULL,
    lamp_rmu_in1 BOOLEAN NOT NULL,
    sw_in1_comment TEXT,
    sw_in2 BOOLEAN NOT NULL,
    sw_ground_in2 BOOLEAN NOT NULL,
    lamp_rmu_in2 BOOLEAN NOT NULL,
    sw_in2_comment TEXT,
    sw1_out1 BOOLEAN NOT NULL,
    sw2_out1 BOOLEAN NOT NULL,
    sw_ground_out1 BOOLEAN NOT NULL,
    lamp_rmu_out1 BOOLEAN NOT NULL,
    sw_out1_comment TEXT,
    sw1_out2 BOOLEAN NOT NULL,
    sw2_out2 BOOLEAN NOT NULL,
    sw_ground_out2 BOOLEAN NOT NULL,
    lamp_rmu_out2 BOOLEAN NOT NULL,
    sw_out2_comment TEXT,
    pressure BOOLEAN NOT NULL,
    -- tr temp 
    tr_ch1 FLOAT NOT NULL,
    tr_ch2 FLOAT NOT NULL,
    tr_ch3 FLOAT NOT NULL,
    tr_room_temp FLOAT NOT NULL,
    tr_room_hum FLOAT NOT NULL,
    tr_temp_comment TEXT,
    -- fan status 
    fan_status BOOLEAN,
    tr_temp_alarm BOOLEAN,
    tr_temp_trip BOOLEAN,
    fan_tr_comment TEXT,
    -- tr memter
    meter FLOAT NOT NULL,
    l1 FLOAT NOT NULL,
    l2 FLOAT NOT NULL,
    l3 FLOAT NOT NULL,
    i1 FLOAT NOT NULL,
    i2 FLOAT NOT NULL,
    i3 FLOAT NOT NULL,
    pf FLOAT NOT NULL,
    main_mcb BOOLEAN,
    db1 BOOLEAN,
    db2 BOOLEAN,
    mcb1 BOOLEAN,
    mcb2 BOOLEAN,
    capbank BOOLEAN,
    status_capbank BOOLEAN,
    -- fan 
    mode_fan BOOLEAN,
    fan1 BOOLEAN,
    fan2 BOOLEAN,
    fan3 BOOLEAN,
    fan4 BOOLEAN,
    fan_ver_comment TEXT,
    -- meter TOU
    meter_TOU FLOAT NOT NULL,
    onpeak FLOAT NOT NULL,
    offpeak FLOAT NOT NULL,
    all_onpeak FLOAT NOT NULL,
    all_offpeak FLOAT NOT NULL,   
    -- create time
    created_at TIMESTAMP NOT NULL,
    updated_at TIMESTAMP NOT NULL,
    -- user 
    user_id INT,
    FOREIGN KEY (user_id) REFERENCES users(user_id)  
    confirm_checklist INT ,
    FOREIGN KEY (confirm_checklist) REFERENCES users(user_id)  
);

CREATE TABLE checklistgenerator (
    id INT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
    generator_name VARCHAR(255),
    --before start
    ats_meter_unit FLOAT,
    around_check BOOLEAN,
    batt_check BOOLEAN,
    batt_connection_check BOOLEAN,
    volt_batt_check FLOAT,
    fuel_value_check FLOAT,
    fuel_leak_check BOOLEAN,
    oil_value_check BOOLEAN,
    oil_leak_check BOOLEAN,
    cooler_check BOOLEAN,
    filter_cooler_check BOOLEAN,
    fan_cooler_check BOOLEAN,
    cooler_leak_check BOOLEAN,
    airduct_check BOOLEAN,
    airfilter_condition_check BOOLEAN,
    electic_junction_check BOOLEAN,
    electic_insulation_check BOOLEAN,
    ground_check BOOLEAN,
    electic_panel_check BOOLEAN,
    time_start TIMESTAMP,
    engine_check BOOLEAN,
    --after start
    afterstart_junction_check BOOLEAN,
    afterstart_insulation_check BOOLEAN,
    l1 FLOAT,
    l2 FLOAT,
    l3 FLOAT,
    i1 FLOAT,
    i2 FLOAT,
    i3 FLOAT,
    frequency FLOAT,
    rpm FLOAT,
    voltbatt FLOAT,
    enginetemp FLOAT,
    cooldown FLOAT,
    comment TEXT,
    -- create time
    created_at TIMESTAMP NOT NULL,
    updated_at TIMESTAMP NOT NULL,
    -- user 
    user_id INT,
    FOREIGN KEY (user_id) REFERENCES users(user_id)  
);