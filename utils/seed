    backupvm_c_free FLOAT NOT NULL,
    backupvm_c_percent FLOAT NOT NULL,
    backupvm_d_total FLOAT NOT NULL,
    backupvm_d_free FLOAT NOT NULL,
    backupvm_d_percent FLOAT NOT NULL,
    backupvm_synctime BOOLEAN NOT NULL,
